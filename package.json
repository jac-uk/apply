{
  "name": "application-form",
  "version": "1.71.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode develop",
    "preview": "vite preview",
    "serve": "vite --mode develop",
    "staging:serve": "vite --mode staging",
    "production:serve": "vite --mode production",
    "develop:build": "vite build --mode develop;",
    "staging:build": "vite build --mode staging",
    "production:build": "vite build --mode production",
    "develop:deploy": "vite build --mode develop; firebase deploy --only hosting:develop-admin-app -P develop",
    "staging:deploy": "vite build --mode staging; firebase deploy --only hosting:staging-admin-app -P staging",
    "production:deploy": "vite build --mode production; firebase deploy --only hosting:production-admin-app -P production",
    "develop:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:develop-candidate-app -P develop",
    "staging:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:staging-candidate-app -P staging",
    "production:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:production-candidate-app -P production",
    "test": "vite test:unit",
    "test:unit": "vite test:unit",
    "test-ci": "vite test:unit --ci --runInBand --silent --forceExit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint-ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --no-fix --ignore-path .gitignore",
    "test:coverage": "TZ='Europe/London' vue-cli-service test:unit --coverage"
  },
  "dependencies": {
    "@jac-uk/jac-kit": "3.0.23",
    "@ministryofjustice/frontend": "0.2.4",
    "@rollup/plugin-inject": "^5.0.3",
    "@sentry/tracing": "^7.64.0",
    "@sentry/vue": "^7.64.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compat": "^3.3.4",
    "autoprefixer": "^10.4.15",
    "firebase": "^8.10.1",
    "firebaseui": "^4.8.0",
    "govuk-frontend": "^3.12.0",
    "mitt": "^3.0.1",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-browser-detect-plugin": "^0.1.18",
    "vue-dompurify-html": "^4.1.4",
    "vue-gtag": "^2.0.1",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.3.4",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "clone": "^2.1.2",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "firebase-mock": "^2.3.2",
    "glob-parent": ">=5.1.2",
    "highlight.js": ">=10.4.1",
    "is-svg": ">=4.3.0",
    "jest-extended": "^0.11.5",
    "node-fetch": ">=2.6.7",
    "postcss": "8.2.15",
    "sass": "^1.32.13",
    "vite-plugin-eslint": "^1.8.1",
    "webpack": "^5.88.2",
    "y18n": ">=4.0.1"
  },
  "engines": {
    "node": "v16.15.1",
    "npm": "^8.11.0"
  }
}
