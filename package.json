{
  "name": "application-form",
  "version": "1.64.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode develop",
    "develop:serve": "vue-cli-service serve --mode develop",
    "staging:serve": "vue-cli-service serve --mode staging",
    "production:serve": "vue-cli-service serve --mode production",
    "develop:build": "vue-cli-service build  --mode develop",
    "staging:build": "vue-cli-service build --mode staging",
    "production:build": "vue-cli-service build --mode production",
    "develop:deploy": "vue-cli-service build --mode develop; firebase deploy --only hosting:develop-candidate-app -P develop",
    "staging:deploy": "vue-cli-service build --mode staging; firebase deploy --only hosting:staging-candidate-app -P staging",
    "production:deploy": "vue-cli-service build --mode production; firebase deploy --only hosting:production-candidate-app -P production",
    "develop:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:develop-candidate-app -P develop",
    "staging:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:staging-candidate-app -P staging",
    "production:deploy-service-offline": "rm -rf dist && mkdir dist && cp service-offline/index.html dist/index.html && firebase deploy --only hosting:production-candidate-app -P production",
    "lint": "vue-cli-service lint",
    "lint-ci": "vue-cli-service lint --no-fix",
    "test": "TZ='Europe/London' vue-cli-service test:unit",
    "test:coverage": "TZ='Europe/London' vue-cli-service test:unit --coverage",
    "test-ci": "TZ='Europe/London' vue-cli-service test:unit --ci --runInBand"
  },
  "dependencies": {
    "@jac-uk/jac-kit": "0.0.49",
    "@ministryofjustice/frontend": "0.2.4",
    "@sentry/tracing": "^7.13.0",
    "@sentry/vue": "^7.13.0",
    "core-js": "^3.23.2",
    "firebase": "^8.10.1",
    "firebaseui": "^4.8.0",
    "govuk-frontend": "^3.12.0",
    "vue": "^2.6.12",
    "vue-browser-detect-plugin": "^0.1.13",
    "vue-dompurify-html": "^2.6.0",
    "vue-gtag": "^1.11.0",
    "vue-recaptcha-v3": "^1.9.0",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "vuexfire": "^3.2.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.8",
    "@vue/babel-preset-app": "^5.0.6",
    "@vue/cli-plugin-eslint": "^5",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.3.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "clone": "^2.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "firebase-mock": "^2.3.2",
    "glob-parent": ">=5.1.2",
    "highlight.js": ">=10.4.1",
    "is-svg": ">=4.3.0",
    "jest-extended": "^0.11.5",
    "node-fetch": ">=2.6.7",
    "postcss": "8.2.15",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "y18n": ">=4.0.1"
